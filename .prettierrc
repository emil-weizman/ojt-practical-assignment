{
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none",
    "endOfLine": "auto",
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "printWidth": 140,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "tabWidth": 2,
    "useTabs": false,
    "vueIndentScriptAndStyle": false,
    "rangeStart": 0
}